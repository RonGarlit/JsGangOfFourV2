      };
  }

  function Vendor(name) {
      this.name = name;
      this.say = function () {
          log.add("I am vendor " + name);
      };
  }

  function VendorFactory() {
      this.create = function(name) {
          return new Vendor(name);
      };
  }

  // log helper
  var log = (function () {
      var log = "";
      return {
          add: function (msg) { log += msg + "\n"; },
          show: function () { alert(log); log = ""; }
